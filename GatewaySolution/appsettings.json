{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "Secret": "THIS IS USED TO SIGN AND VERIFY JWT TOKENS, REPLACE IT WITH YOUR OWN SECRET",
    "Issuer": "auth-api",
    "Audience": "client"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Brand",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7003
        }
      ],
      "UpstreamPathTemplate": "/api/Brand",
      "UpstreamHttpMethod": [ "Get" ],
      "AddHeadersToRequest": {
        "Access-Control-Allow-Origin": "http://localhost:4200",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
        "Access-Control-Allow-Headers": "*"
      }
    },
    {
      "DownstreamPathTemplate": "/api/Brand/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7003
        }
      ],
      "UpstreamPathTemplate": "/api/Brand/{id}",
      "UpstreamHttpMethod": [ "Get" ],
      "AddHeadersToRequest": {
        "Access-Control-Allow-Origin": "http://localhost:4200",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
        "Access-Control-Allow-Headers": "*"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7777"
  }
}
